{
  "name": "suenerds/arcanist-rest-api-renderer",
  "description": "A package containing the components for our formbuilder using arcanist",
  "type": "library",
  "license": "MIT",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/suenert/arcanist"
    }
  ],
  "autoload": {
    "psr-4": {
      "Suenerds\\ArcanistRestApiRenderer\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Suenerds\\ArcanistRestApiRenderer\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^6.0 || ^7.0 || ^8.0",
    "phpunit/phpunit": "^10.5"
  },
  "require": {
    "laravel-arcanist/arcanist": "^1.0"
  }
}
